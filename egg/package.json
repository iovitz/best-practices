{
  "name": "egg",
  "version": "1.0.0",
  "description": "egg best practice",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^3",
    "egg-router-plus": "^2.0.0",
    "egg-scripts": "^2",
    "egg-socket.io": "^4.1.6",
    "egg-swagger-doc-feat": "^2.2.14",
    "egg-validate": "^2.0.2"
  },
  "devDependencies": {
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=anyway-server  --sticky",
    "stop": "egg-scripts stop --title=anyway-server",
    "dev": "egg-bin dev --sticky",
    "debug": "egg-bin debug --sticky",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "ci": {
    "version": "16, 18",
    "type": "github"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT"
}
