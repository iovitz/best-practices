{
  "name": "nest-server",
  "version": "0.0.1",
  "private": true,
  "description": "A NestJS Project.",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "pi": "pnpm install",
    "pi:prod": "pnpm install --prod",
    "start": "nest start",
    "start:debug": "cross-env NODE_ENV=production nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "yac": "node_modules/.bin/httpyac -e development -o body ./rest.http",
    "drizzle:mysql": "dotenv -e .env.development -- drizzle-kit --config ./drizzle/mysql.ts",
    "drizzle:sqlite": "dotenv -e .env.development -- drizzle-kit --config ./drizzle/sqlite.ts",
    "prisma:sqlite:generate": "dotenv -e .env.development -- prisma generate --schema=./prisma/sqlite.prisma",
    "prisma:sqlite:sync": "dotenv -e .env.development -- prisma db push --schema=./prisma/sqlite.prisma",
    "prisma:mysql:generate": "dotenv -e .env.development -- prisma generate --schema=./prisma/mysql.prisma",
    "prisma:mysql:sync": "dotenv -e .env.development -- prisma db push --schema=./prisma/mysql.prisma",
    "update:dependence": "pnpx npm-check-updates"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "2.0.2",
    "@nestjs/cache-manager": "3.0.0",
    "@nestjs/common": "11.0.11",
    "@nestjs/core": "11.0.11",
    "@nestjs/event-emitter": "3.0.1",
    "@nestjs/mongoose": "11.0.0",
    "@nestjs/platform-express": "11.0.11",
    "@nestjs/platform-socket.io": "11.0.11",
    "@nestjs/swagger": "11.0.6",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/websockets": "11.0.11",
    "@prisma/client": "6.5.0",
    "@prisma/client-mysql": "file:./prisma/clients/client-mysql",
    "@prisma/client-sqlite": "file:./prisma/clients/client-mysql",
    "ansis": "^3.17.0",
    "bcrypt": "5.1.1",
    "better-sqlite3": "11.8.1",
    "cache-manager": "6.4.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.7",
    "crypto-js": "4.2.0",
    "drizzle-orm": "0.40.0",
    "ejs": "3.1.10",
    "ioredis": "5.6.0",
    "lodash": "4.17.21",
    "mime-types": "3.0.0",
    "moment": "2.30.1",
    "mongoose": "8.12.1",
    "mysql2": "3.13.0",
    "pako": "2.1.0",
    "rc": "1.2.8",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "safe-stable-stringify": "2.5.0",
    "socket.io": "4.8.1",
    "statuses": "2.0.1",
    "superagent": "10.2.0",
    "svg-captcha": "1.4.0",
    "triple-beam": "1.4.1",
    "typeorm": "0.3.21",
    "ua-parser-js": "2.0.2",
    "ulid": "^2.3.0",
    "winston": "3.17.0",
    "winston-daily-rotate-file": "5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.10.1",
    "@nestjs/cli": "11.0.5",
    "@nestjs/schematics": "11.0.2",
    "@nestjs/testing": "11.0.11",
    "@types/bcrypt": "5.0.2",
    "@types/better-sqlite3": "7.6.12",
    "@types/cookie-parser": "1.4.8",
    "@types/crypto-js": "4.2.2",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/json-stringify-safe": "5.0.3",
    "@types/lodash": "4.17.16",
    "@types/mime-types": "2.1.4",
    "@types/node": "22.13.10",
    "@types/pako": "2.0.3",
    "@types/rc": "1.2.4",
    "@types/statuses": "2.0.5",
    "@types/superagent": "8.1.9",
    "@types/supertest": "6.0.2",
    "@types/triple-beam": "1.3.5",
    "@types/ua-parser-js": "0.7.39",
    "cross-env": "7.0.3",
    "dotenv-cli": "8.0.0",
    "drizzle-kit": "0.30.5",
    "eslint": "9.22.0",
    "httpyac": "6.16.6",
    "jest": "29.7.0",
    "npm-check-updates": "^17.1.15",
    "prisma": "6.5.0",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.6",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
